                <maximum>0</maximum>
                <unit></unit>
            </parameter>
            <parameter name="latitude">
                <startbit>48</startbit>
                <length>32</length>
                <byteorder>Intel</byteorder>
                <valuetype>unsigned</valuetype>
                <factor>0.000001</factor>
                <offset>0</offset>
                <minimum>0</minimum>
                <maximum>0</maximum>
                <unit></unit>
            </parameter>
        </sensor>
