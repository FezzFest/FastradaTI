                <offset>0</offset>
                <minimum>0</minimum>
                <maximum>359.999</maximum>
                <unit>Km/h</unit>
            </parameter>
            <parameter name="Gear">
                <startbit>32</startbit>
                <length>8</length>
                <byteorder>Intel</byteorder>
                <valuetype>unsigned</valuetype>
                <factor>1</factor>
                <offset>0</offset>
                <minimum>0</minimum>
                <maximum>255</maximum>
