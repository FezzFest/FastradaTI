                <maximum>65535</maximum>
                <unit>r/min</unit>
            </parameter>
            <parameter name="Throttle_position">
                <startbit>32</startbit>
                <length>16</length>
                <byteorder>Intel</byteorder>
                <valuetype>unsigned</valuetype>
                <factor>0.0015259</factor>
                <offset>0</offset>
                <minimum>0</minimum>
                <maximum>99.9999</maximum>
                <unit>%</unit>
            </parameter>
            <parameter name="Engine_Temp">
                <startbit>48</startbit>
                <length>16</length>
                <byteorder>Intel</byteorder>
                <valuetype>signed</valuetype>
                <factor>0.0030518</factor>
                <offset>50</offset>
                <minimum>-50.0014</minimum>
                <maximum>149.998</maximum>
                <unit>Â°C</unit>
            </parameter>
        </sensor>
        <sensor id="257" name="Car_Data">
